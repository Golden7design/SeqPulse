          "rawSql": "WITH cohort AS (\n  SELECT p.id AS project_id, date_trunc('day', p.created_at) AS day, p.created_at\n  FROM projects p\n  WHERE p.created_at BETWEEN $__timeFrom() AND $__timeTo()\n),\nfirst_verdict AS (\n  SELECT\n    c.project_id,\n    c.day,\n    c.created_at,\n    MIN(dv.created_at) AS first_verdict_at\n  FROM cohort c\n  LEFT JOIN deployments d ON d.project_id = c.project_id\n  LEFT JOIN deployment_verdicts dv ON dv.deployment_id = d.id\n  GROUP BY c.project_id, c.day, c.created_at\n),\ndaily AS (\n  SELECT\n    day,\n    COUNT(*) AS total_projects,\n    COUNT(*) FILTER (\n      WHERE first_verdict_at IS NOT NULL\n        AND first_verdict_at <= created_at + interval '72 hour'\n    ) AS projects_with_verdict\n  FROM first_verdict\n  GROUP BY day\n)\nSELECT\n  day AS \"time\",\n  CASE WHEN total_projects = 0 THEN 0\n       ELSE ROUND(100.0 * projects_with_verdict / total_projects, 2)\n  END AS value\nFROM daily\nORDER BY day;",
